    "@types/react-router-config": "^5.0.3",
    "ahooks": "^2.10.9",
    "antd": "^4.16.13",
    "less": "^4.1.1",
    "lodash": "^4.17.21",
    "mobx": "^6.3.3",
    "mobx-react": "^7.2.0",
